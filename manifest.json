{
  "manifest_version": 2,
  "name": "Close duplicate tabs",
  "version": "0.2",
  "browser_action": {
      "default_title": "Close duplicate tabs",
      "default_icon": {
        "48": "icon.svg"
    }
  },
  "background": {
      "scripts": ["background.js"]
  },
  "icons": {
      "48": "icon.svg"
  },
  "permissions": [
      "cookies",
      "notifications",
      "tabs"
  ]
}
